<Project>
    <ItemGroup>
        <IkvmNetReferenceAssemblies Remove="@(IkvmNetReferenceAssemblies)" />
        <IkvmNetReferenceAssemblies Include="$(MSBuildThisFileDirectory)\..\netref\**\*" />
        <None Include="@(IkvmNetReferenceAssemblies)" Visible="false">
            <TargetPath>netref\%(RecursiveDir)%(Filename)%(Extension)</TargetPath>
            <PackagePath>netref\%(RecursiveDir)%(Filename)%(Extension)</PackagePath>
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
            <Pack>true</Pack>
        </None>
    </ItemGroup>
</Project>
